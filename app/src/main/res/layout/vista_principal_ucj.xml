<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    android:padding="16dp"
    tools:context=".PrincipalUCJ">

   <Button
       android:id="@+id/cuentasPagadas"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="@string/cuentas_pagadas_strings"
       />

    <Button
        android:id="@+id/cuentasSinPagar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cuentas_sin_pagar_strings"
        android:layout_marginTop="30dp"
        />



    <Button
        android:id="@+id/LogOutButtonCajero"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cerrar_sesion_strings"
        android:layout_marginTop="100dp"
        />


</LinearLayout>